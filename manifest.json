{
  "manifest_version": 2,

  "name": "Crowdin Translate Plugin",
  "description": "Some explanation",
  "version": "1.0",

  "browser_action": {
    "default_icon": "crowdin-logo-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["0jipt.js", "jipt.js"]
    }
  ]
}
